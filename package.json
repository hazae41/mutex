{
  "type": "module",
  "name": "@hazae41/mutex",
  "version": "2.2.1",
  "description": "Rust-like Mutex for TypeScript",
  "homepage": "https://github.com/hazae41/mutex",
  "repository": "github:hazae41/mutex",
  "author": "hazae41",
  "license": "MIT",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/types/index.d.ts",
  "sideEffects": false,
  "files": [
    "./dist/esm",
    "./dist/cjs",
    "./dist/types"
