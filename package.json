    "@hazae41/rimraf": "^1.0.1",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/node": "^24.0.1",
    "rollup": "^4.43.0",
    "rollup-plugin-dts": "^6.2.1",
    "rollup-plugin-node-externals": "^8.0.0",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3"
  },
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs"
    }
  },
  "keywords": [
    "mutex",
    "promise",
    "manual",
    "lock",
    "wait",
    "semaphore",
    "async",
    "signal",
    "typescript",
    "esmodules",
    "tested",
    "unit-tested"
  ]
