{
  "type": "module",
  "name": "@hazae41/mutex",
  "version": "3.0.0",
  "description": "Rust-like Mutex for TypeScript",
  "repository": "github:hazae41/mutex",
  "author": "hazae41",
  "scripts": {
    "examine": "deno lint ./src && deno check ./src && deno test ./src",
    "prepare": "rm -rf ./out && tsc && tscousin",
    "version": "deno -RW ./x.vsync.ts && git add deno.json",
    "synchro": "deno -RW ./x.dsync.ts && deno install"
  },
  "files": [
    "./out"
  ],
  "exports": {
    ".": {
      "types": "./out/mod.d.ts",
      "import": "./out/mod.js"
    }
  },
  "devDependencies": {
    "@hazae41/phobos": "^2.0.10",
    "@hazae41/tscousin": "^1.0.9",
    "typescript": "^5.9.2"
  },
  "keywords": [
    "mutex",
    "promise",
    "manual",
    "lock",
    "wait",
    "semaphore",
    "async",
    "signal",
    "typescript",
    "esmodules",
    "tested",
    "unit-tested"
  ]
}
